--Function diamonds() is my function that sucks in diamonds and drops slot 1
function diamonds()
  for x = 1, 9 do
    turtle.suck()
  end
  for b = 1, 16 do
   local data = turtle.getItemDetail(1)
      if data then
        turtle.select(1)
        turtle.drop(4)
  end
end
end
 
